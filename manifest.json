{
	"manifest_version": 2,
	"name": "alibaba jp Manual Opener",
	"version": "0.0.1",
	"description": "",
	"content_scripts": [{
		"matches": ["http://*/*","https://*/*"],
		"js": ["script.js"]
	}],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"16": "16x16.png"
		}
	},
	"permissions": [
		"tabs",
		"background",
		"https://*/*",
		"http://*/*"
  ]
}
